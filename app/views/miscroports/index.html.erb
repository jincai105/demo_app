<h1>Listing miscroports</h1>

<table>
  <tr>
    <th>Content</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @miscroports.each do |miscroport| %>
  <tr>
    <td><%= miscroport.content %></td>
    <td><%= miscroport.user_id %></td>
    <td><%= link_to 'Show', miscroport %></td>
    <td><%= link_to 'Edit', edit_miscroport_path(miscroport) %></td>
    <td><%= link_to 'Destroy', miscroport, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

